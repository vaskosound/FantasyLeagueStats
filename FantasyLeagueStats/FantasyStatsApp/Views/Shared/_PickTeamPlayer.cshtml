@model FantasyStatsApp.Models.GameModels.PlayerGameViewModel
@if (Model.Name != null)
{
    string src = "/Images/" + @Model.Team + ".png";
    <img src=@src class="kit" alt=@Model.Team title=@Model.Team />
    <span class="name btn-primary">@Model.Name</span>
    <span class="price btn-primary">@Model.AgainstTeamInGameweek</span>
}
else
{
    <img src="~/Images/empty.png" class="kit" />
}