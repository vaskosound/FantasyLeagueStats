@model IEnumerable<FantasyStatsApp.Models.GameModels.GameBasicViewModel>
@{
    ViewBag.Title = "Join Games";
}

<h2>ViewBag.Title</h2>
<ul>
    @foreach (var game in @Model)
    {
        <li>
            @Html.ActionLink(@game.Name, "JoinGame", "GameInfo", new { id = @game.Id }, null)
        </li>
    }
</ul>

